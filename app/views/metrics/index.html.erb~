<h1>Listing metrics</h1>

<table border="1" width="100%">
  <tr>
    <th>Status Ids</th>
    <th>Text</th>
    <th>Accounts used</th>
    <th>Followers count</th>
    <th>Time Taken(in milliseconds)</th>
    <th>Failures</th>
    <th>Created at</th>
  </tr>

<% @metrics.each do |metric| %>
  <tr>
    <td><%= metric.Status_id %></td>
    <td><%= metric.Text %></td>
    <td><%= metric.Accounts_Used %></td>
    <td><%= metric.Followers_Count %></td>
    <td><%= metric.Time %></td>
    <td><%= metric.Failures %></td>
    <td><%= metric.created_at %></td>
  </tr>
<% end %>
</table>

